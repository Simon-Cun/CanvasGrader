{
  "manifest_version": 3,
  "name": "Canvas Grade Calculator",
  "version": "1.0",
  "description": "Calculates your canvas grade based on assignments.",
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://elearn.ucr.edu/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
